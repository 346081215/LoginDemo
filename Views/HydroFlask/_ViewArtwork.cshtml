@using System.Data



  <div class="row-fluid">
        <p class="select" style="font-size:15px;">
            Order No. = [ @ViewBag.oos_custorderno ],  Maxim No. = [ @ViewBag.oos_orderno ].
        </p>
        <div id="Gridview" class="dataTables_wrapper form-inline" role="grid">
            <table class="table table-striped table-bordered table-hover table-full-width dataTable" aria-describedby="sample_2_info">
                <thead>
                    <tr class="head">
                        <th class="center">Seq</th>
                        <th class="center">Item</th>
                        <th class="center">Artwork File</th>
                        <th class="center">View Artwork</th>
                    </tr>
                </thead>
                <tbody class="OrderDetail">
                    @{
                        var orderdata = ViewBag.OrderData as DataTable;
                        @*< tr>
                            <td>1</td>
                            <td>Care Label</td>
                            <td>131061231310524617.pdf</td>
                            <td><a href="http://192.168.3.58/WebDocuments/ArtWork/MAM1604260014/131061231310524617.pdf" target="_blank">View</a></td>
                        </tr>*@
                    for (int i = 0; i < orderdata.Rows.Count; i++)
                    {
                        var row = orderdata.Rows[i];
                        <tr>
                            <td class="center">@(i + 1)</td>
                            <td class="center">@row["oos_prodalias"].ToString()</td>
                            <td class="center">@row["oos_filename"].ToString()</td>
                            @if (row["oos_filename"].ToString().Trim() != "")
                            {
                                <td class="center"><a href=@row["oos_file"].ToString() target="_blank">View</a></td>
                            }
                            else
                            {
                                <td class="center">View</td>
                            }
                        </tr>
                    }
                    }
                </tbody>
            </table>
        </div>
   </div>





