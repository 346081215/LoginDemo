@model IEnumerable<Maxim.Model.BaseModel.ViewShippingOrder>
<div class="row-fluid">
    <div id="Gridview" class="dataTables_wrapper form-inline" role="grid">
        <table class="table table-striped table-bordered table-hover table-full-width dataTable" id="Order" aria-describedby="sample_2_info">
            <thead>
                <tr role="row">
                    <th class="center">Seq</th>
                    <th class="center">PO#</th>
                    <th class="center">Maxim OOS NO.</th>
                    <th class="center">Release_Qty_Shipped</th>
                    <th class="center">Shiporder No</th>
                    <th class="center">Release_Actual_XFD</th>
                    <th class="center">Release_Days_Off_Track</th>
                    <th class="center">Release_ETA</th>
                    <th class="center">Company</th>
                    <th class="center">Address</th>
                    <th class="center">ShippedDate</th>
                    <th class="center">Contacter</th>
                    <th class="center">Telephone</th>
                    <th class="center">FreightCompany</th>
                    <th class="center">FreightNo</th>
            </thead>
            <tbody role="alert" aria-live="polite" aria-relevant="all">
                @foreach (var item in Model)
            {
                    <tr>
                        <td class="center">@Html.DisplayFor(modelItem => item.Seq)</td>
                        <td class="center">@Html.DisplayFor(modelItem => item.OrderNO)</td>
                        <td class="center">@Html.DisplayFor(modelItem => item.MaximNO)</td>
                        <td class="center">@Html.DisplayFor(modelItem => item.Release_Qty_Shipped)</td>
                        <td class="center">@Html.DisplayFor(modelItem => item.OOS_shiporderno)</td>
                        <td class="center"></td>
                        <td class="center"></td>
                        <td class="center"></td>
                        <td class="center">@Html.DisplayFor(modelItem => item.Company)</td>
                        <td class="center">@Html.DisplayFor(modelItem => item.Address)</td>
                        <td class="center">@Html.DisplayFor(modelItem => item.ShippedDate)</td>
                        <td class="center">@Html.DisplayFor(modelItem => item.Contacter)</td>
                        <td class="center">@Html.DisplayFor(modelItem => item.Telephone)</td>
                        <td class="center">@Html.DisplayFor(modelItem => item.FreightCompany)</td>
                        <td class="center"><a href="@Html.DisplayFor(modelItem => item.FreightCompanyUrl)" target="_blank">@Html.DisplayFor(modelItem => item.FreightNo)</a></td>
                    </tr>
                }

            </tbody>
        </table>
    </div>
</div>
