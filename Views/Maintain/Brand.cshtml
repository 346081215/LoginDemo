@using System.Data;
@using Maxim.Model.BaseModel
@model Brand_Model
@{
    ViewBag.Title = "Brand";
    Layout = null;
}
<script type="text/javascript" src="~/scripts/Maintain/Brand.js"></script>
<div class="row-fluid">
    <div class="span12">
        <div class="portlet box " id="xm">
            <div class="portlet-title">
                <div class="caption">
                    <em class="red-em" id="step1">Brand</em>
                    <em class="red-em" id="step2" style="display:none">AddBrand</em>
                    <em class="red-em" id="step3" style="display:none">EditBrand</em>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="all">
        BrandId:@Html.DropDownListFor(x => x.BrandId_sel, Model.Brandlist, new { @class = "m-wrap span3" })
        <button type="submit" class="btn" id="btnQuery" style="border: 1px solid ;width: 170px;margin-left:0;" onclick="SelectAll()">Query</button>
        <button type="button" class="btn" id="newBrand" name="newBrand" onclick="return CreatenewBrand();">NewBrand</button>
        <div id="Gridview" class="dataTables_wrapper form-inline" role="grid">
            <table id="ShowDetailShowDataSetting" class="table table-striped table-bordered table-hover table-full-width dataTable" aria-describedby="sample_2_info">
                <thead>
                    <tr class="head">
                        <th class="center">BrandId</th>
                        <th class="center">Name</th>
                        <th class="center">CodeName</th>
                        <th class="center">ContactAddress</th>
                        <th class="center">ContactPerson</th>
                        <th class="center">ContactTel</th>
                        <th class="center">Country</th>
                        <th class="center">CountryCode</th>
                        <th class="center">LogoUrl</th>
                        <th class="center">ParentBrandId</th>
                        <th class="center">IsEnabled</th>
                        <th class="center">EnableERPFunc</th>
                        <th class="center">EnableBigVndFunc</th>
                        <th class="center">EnableReviseQtyFunc</th>
                        <th class="center">操作</th>
                    </tr>
                </thead>
                <tbody id="BrandDetail"></tbody>
            </table>
            @*分页*@
            <div class="span4">
                <div class="mypage">
                    <ul>
                        <li id="PageIndex"><<</li>
                        <li id="Pageup"><</li>
                        <li><input type="text" id="nowpage" value="1" /></li>
                        <li>/</li>
                        <li><i id="TotalPageCount">0</i></li>
                        <li id="Pagewown">></li>
                        <li id="PageLast">>></li>
                        <li id="PageGO">GO</li>
                        <li><input id="pc" name="pc" type="hidden" value=1><input id="PageTotalCount" name="PageTotalCount" type="hidden" value=1></li>
                    </ul>
                </div>
            </div>

        </div>
    </div>
