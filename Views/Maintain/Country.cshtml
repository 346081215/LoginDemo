@{
    ViewBag.Title = "Country";
    Layout = null;
}
<script src="~/scripts/Maintain/Country.js"></script>

<!DOCTYPE html>
<div id="all">
    <h2>Country</h2>
    <span>oos_countryEName:</span> <input type="text" id="inputCountryname" value="@ViewBag.name" placeholder="国家 " />
    <button type="submit" class="btn" id="btnQuery" style="border: 1px solid ;width: 170px;margin-left:0;" onclick="SelectCountrybyName()">Query</button>
    <button type="button" class="btn" id="newCountry" name="newCountry" onclick="return CreatenewCountry();">NewCountry</button>

    <div id="Gridview" class="dataTables_wrapper form-inline" role="grid">
        <table id="ShowShipping" class="table table-striped table-bordered table-hover table-full-width dataTable" aria-describedby="sample_2_info">
            <thead>
                <tr class="head">
                    <th class="center">oos_countrycode</th>
                    <th class="center">oos_countryEName</th>
                    <th class="center">oos_countryCName</th>
                    <th class="center">TelephoneCode</th>
                    <th class="center">TimeDiff</th>
                    <th class="center">WithHoldTax</th>
                    <th class="center">IsEnabled</th>
                    <th class="center">ModifiedBy</th>
                    <th class="center">ModifiedOn</th>
                    <th class="center">Notes</th>
                    <th class="center">oos_countryFullname</th>
                    <td class="center">oos_countryMEname</td>
                    <td class="center"></td>
                </tr>
            </thead>
            <tbody id="CountryDetail"></tbody>
        </table>
        @*分页*@
        <div class="span4">
            <div class="mypage">
                <ul>
                    <li id="PageIndex"><<</li>
                    <li id="Pageup"><</li>
                    <li><input type="text" id="nowpage" value="1" /></li>
                    <li>/</li>
                    <li><i id="TotalPageCount">0</i></li>
                    <li id="Pagewown">></li>
                    <li id="PageLast">>></li>
                    <li id="PageGO">GO</li>
                    <li><input id="pc" name="pc" type="hidden" value=1><input id="PageTotalCount" name="PageTotalCount" type="hidden" value=1></li>
                </ul>
            </div>
        </div>

    </div>
</div>
<div id="InsertCountry" style="display:none;">
    @*第一行*@
    <div class="row-fluid">
        <div class="span2 span2_hide">
            <div class="control-group">
                <label class="control-label" for="firstName"></label>
                <div class="controls">
                </div>
            </div>
        </div>
        <div class="span4 ">
            <span>countrycode:</span>
            <input type="text" id="oos_countrycode" name="oos_countrycode" class="m-wrap span8" />
        </div><div class="span4 ">
            <span>countryEName:</span>
            <input type="text" id="oos_countryEName" name="oos_countryEName" class="m-wrap span8" />
        </div>
    </div>
    @*第二行*@
    <div class="row-fluid">
        <div class="span2 span2_hide">
            <div class="control-group">
                <label class="control-label" for="firstName"></label>
                <div class="controls">
                </div>
            </div>
        </div>
        <div class="span4 ">
            <span>countryCName:</span>
            <input type="text" id="oos_countryCName" name="oos_countryCName" class="m-wrap span8" />
        </div><div class="span4 ">
            <span>TelephoneCode:</span>
            <input type="text" id="TelephoneCode" name="TelephoneCode" class="m-wrap span8" />
        </div>
    </div>
    @*第三行*@
    <div class="row-fluid">
        <div class="span2 span2_hide">
            <div class="control-group">
                <label class="control-label" for="firstName"></label>
                <div class="controls">
                </div>
            </div>
        </div>
        <div class="span4 ">
            <span>Notes:</span>
            <input type="text" id="Notes" name="Notes" class="m-wrap span8" />
        </div>
        <div class="span4 ">
            <span>countryFullname:</span>
            <input type="text" id="oos_countryFullname" name="oos_countryFullname" class="m-wrap span8" />
        </div>
    </div>
    @*第四行*@
    <div class="row-fluid">
        <div class="span2 span2_hide">
            <div class="control-group">
                <label class="control-label" for="firstName"></label>
                <div class="controls">
                </div>
            </div>
        </div>
        <div class="span4 ">
            <span>countryMEname:</span>
            <input type="text" id="oos_countryMEname" name="oos_countryMEname" class="m-wrap span8" />
        </div>
    </div>
    @*保存和返回按钮*@
    <div class="row-fluid" id="SaveAt">
        <div class="span3 span2_hide">
        </div>
        <div class="span3">
            <button type="submit" id="SaveCountry" name="SaveCountry" class="btn btn-default" value="Save" onclick="funSaveCountry()">Save</button>
            <button type="button" id="ReturnIndex" onclick="ReturnIndex()" name="ReturnIndex" class="btn btn-default" value="Query">Return</button>
        </div>
    </div>
</div>
<div id="UpdateCountry" style="display:none;">
    @*第一行*@
    <div class="row-fluid">
        <div class="span2 span2_hide">
            <div class="control-group">
                <label class="control-label" for="firstName"></label>
                <div class="controls">
                </div>
            </div>
        </div>
        <div class="span4 ">
            <span>countryEName:</span>
            <input type="text" id="oos_countryEName_u" name="oos_countryEName_u" class="m-wrap span8" />
        </div><div class="span4 ">
            <span>countryCName:</span>
            <input type="text" id="oos_countryCName_u" name="oos_countryCName_u" class="m-wrap span8" />
        </div>
    </div>
    @*第二行*@
    <div class="row-fluid">
        <div class="span2 span2_hide">
            <div class="control-group">
                <label class="control-label" for="firstName"></label>
                <div class="controls">
                </div>
            </div>
        </div>
        <div class="span4 ">
            <span>TelephoneCode:</span>
            <input type="text" id="TelephoneCode_u" name="TelephoneCode_u" class="m-wrap span8" />
        </div>
        <div class="span4 ">
            <span>Notes:</span>
            <input type="text" id="Notes_u" name="Notes_u" class="m-wrap span8" />
        </div>
    </div>
    @*第三行*@
    <div class="row-fluid">
        <div class="span2 span2_hide">
            <div class="control-group">
                <label class="control-label" for="firstName"></label>
                <div class="controls">
                </div>
            </div>
        </div>
        <div class="span4 ">
            <span>countryFullname:</span>
            <input type="text" id="oos_countryFullname_u" name="oos_countryFullname_u" class="m-wrap span8" />
        </div>
        <div class="span4 ">
            <span>countryMEname:</span>
            <input type="text" id="oos_countryMEname_u" name="oos_countryMEname_u" class="m-wrap span8" />
        </div>
    </div>
    @*保存和返回按钮*@
    <div class="row-fluid" id="SaveAt">
        <div class="span3 span2_hide">
        </div>
        <div class="span3">
            <button type="submit" id="SaveCountry_u" name="SaveAccount_u" class="btn btn-default" value="Save" onclick="funSaveCountry_u()">Save</button>
            <button type="button" id="ReturnIndex_u" onclick="ReturnIndex_u()" name="ReturnIndex_u" class="btn btn-default" value="Query">Return</button>
            <input id="EditCountry_u" name="EditCountry_u" type="hidden" />
        </div>
    </div>
</div>
