                        <div class="span2 span2_hide">
                            <div class="control-group">
                                <label class="control-label" for="firstName"></label>
                                <div class="controls">
                                </div>
                            </div>
                        </div>
                        <div class="span4">
                            <span>产品ID:</span>
                            @Html.TextBoxFor(x => x.ProductCode, new { @class = "m-wrap span8", @id = "_ProductCode", @nameddd = "_ProductCode" })<b style="font-size: 20px;color: red;">*</b>
                        </div>
                        <div class="span4">
                            <span>产品状态:</span>
                            @Html.DropDownListFor(x => x._Productstatus, Model.Productstatus, new { @class = "m-wrap span8", @id = "_Productstatus", @name = "_Productstatus" })<b style="font-size: 20px;color: red;">*</b>
                        </div>
                    </div>
                    @*第二行*@
