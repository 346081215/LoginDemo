@using Maxim.Model.BaseModel
@using System.Data

@model ProductEdit
@{ 
    Layout = null;
}

<script src="~/scripts/Maintain/ProductEdit.js"></script>
<div class="row-fluid">
    <div class="span12">
        <div class="portlet box " id="form_wizard_1">
