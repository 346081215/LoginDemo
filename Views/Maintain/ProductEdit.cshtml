                        </div>
                        <div class="span4">
                            <span>创建人:</span>
                            @Html.TextBoxFor(x => x.ProductCreatedBy, new { @class = "m-wrap span8", @id = "_ProductCreatedBy", @name = "_ProductCreatedBy", @readonly = "readonly" })
                        </div>
