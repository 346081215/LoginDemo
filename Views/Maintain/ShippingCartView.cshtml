
@{
    ViewBag.Title = "ShoppingCartView";
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>SpippingQuery</title>
    <meta name="viewport" content="width=device-width" />
    <script src="~/scripts/Maintain/ShippingQuery.js"></script>
</head>

<body>
    <div class="row-fluid">
        <div class="span12">
            <div class="portlet box blue" id="form_wizard_1">
                <div class="portlet-title">
                    <div class="caption">
                        Order- <span class="step-title">Shopping Cart</span>
                    </div>
                </div>
                <div class="portlet-body form">
                    <div action="#" class="form-horizontal" id="submit_form" novalidate="novalidate">
                        <div class="form-wizard">
                            <div class="navbar steps">
                                <div class="navbar-inner">
                                    <ul class="row-fluid nav nav-pills">
                                        <li class="span3 active" id="FirstStep" style="width:32%">
                                            <a href="#tab1" data-toggle="tab" class="step active">
                                                <span class="number">1</span>
                                                <span class="desc"><i class="icon-ok"></i> Select Items</span>
                                            </a>
                                        </li>
                                        <li class="span3" id="SecondStep" style="width:32%">
                                            <a href="#tab2" data-toggle="tab" class="step">
                                                <span class="number">2</span>
                                                <span class="desc"><i class="icon-ok"></i> Cart</span>
                                            </a>
                                        </li>
                                        <li class="span3" id="ThirdStep" style="width:30%">
                                            <a href="#tab4" data-toggle="tab" class="step">
                                                <span class="number">3</span>
                                                <span class="desc"><i class="icon-ok"></i> Order Complete</span>
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div id="bar" class="progress progress-success progress-striped">
                                <div class="bar" style="width: 36%;"></div>
                            </div>

                            <div class="tab-content" style="overflow:hidden;">
                                <div id="Query-order">
                                    <h3 class="form-section" style="text-align:center"></h3>
                                    @*第一行*@
                                    <div class="row-fluid">
                                        <div class="span4 ">
                                            <span>Print Shop:</span>
                                            <select id="tionPriintShop" name="tionPriintShop" type="text" class="m-wrap span8">
                                                <option>--All--</option>
                                            </select>
                                        </div>
                                        <div class="span4 ">
                                            <span>Currency:</span>
                                            <input type="text" id="tionCurrency" name="tionCurrency" class="m-wrap span8" />
                                        </div>
                                    </div>
                                    @*第二行*@
                                    <div class="row-fluid">
                                        <div class="span4 ">
                                            <span>Category:</span>
                                            <select id="tionCategory" name="tionCategory" type="text" class="m-wrap span8">
                                                <option>--All--</option>
                                            </select>
                                        </div>
                                        <div class="span4 ">

                                            <span>AddComments:</span>
                                            <input type="text" id="tionAddComments" name="tionAddComments" class="m-wrap span8" />
                                        </div>
                                    </div>
                                    @*第三行*@
                                    <div class="row-fluid">
                                        <div class="span4 ">
                                            <span>ShipmentType:</span>
                                            <select id="tionShipmentType" name="tionShipmentType" type="text" class="m-wrap span8">
                                                <option>--Please Select--</option>
                                            </select>
                                        </div>
                                        <div class="span4 ">


                                        </div>
                                    </div>
                                </div>
                                                  

                                <div class="tab-pane active" id="tab1">                                                                                                                                          
                                    <div id="Gridview" class="dataTables_wrapper form-inline" role="grid">
                                        <table class="table table-striped table-bordered table-hover table-full-width dataTable" id="ProductTable" aria-describedby="sample_2_info">
                                            <thead>
                                                <tr role="row">
                                                    <th class="center">OrderQty</th>
                                                    <th class="center">Product Sample</th>
                                                    <th class="center">Description</th>
                                                    <th class="center">Currency</th>
                                                    <th class="center">Unit Price（EXW）</th>
                                                    <th class="center">UOM</th>
                                                    <th class="center">MOQ</th>
                                                </tr>
                                            </thead>
                                            <tbody class="OrderDetail" id="Product" role="alert" aria-live="polite" aria-relevant="all"></tbody>
                                        </table>
                                    </div>

                                    @*分页*@
                                    <div class="row-fluid">
                                        <div class="span4">
                                            <div class="mypage">
                                                <ul>
                                                    <li id="PageIndex"><<</li>
                                                    <li id="Pageup"><</li>
                                                    <li><input type="text" id="nowpage" value="1" /></li>
                                                    <li>/</li>
                                                    <li><i id="TotalPageCount">1</i></li>
                                                    <li id="Pagewown">></li>
                                                    <li id="PageLast">>></li>
                                                    <li id="PageGO">GO</li>
                                                    <li><input id="pc" name="pc" type="hidden" value=1><input id="PageTotalCount" name="PageTotalCount" type="hidden" value=1></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row-fluid">
                                        <input type="button" class="btn" id="AddCart" value="Add to Cart" />
                                    </div>
                                    <div id="Gridview" class="dataTables_wrapper form-inline" role="grid">
                                        <table class="table table-striped table-bordered table-hover table-full-width dataTable" id="ShippingCar" aria-describedby="sample_2_info">
                                            <thead>
                                                <tr role="row">
                                                    <th class="center">Remove</th>
                                                    <th class="center">OrderQty</th>
                                                    <th class="center">Description</th>
                                                </tr>
                                            </thead>
                                            <tbody class="OrderDetail" id="CartProduct" role="alert" aria-live="polite" aria-relevant="all"></tbody>
                                        </table>
                                    </div>
                                    <div class="row-fluid">
                                        <div class="row-fluid">
                                            <input type="button" class="btn" id="AddCart" value="Ordering Now" />
                                        </div>
                                        <div class="span4"></div>
                                        <div class="span4"></div>
                                    </div>                                 
                                </div>




                                <div class="tab-pane" id="tab2">

                                    <h3 class="block">Provide your profile details</h3>

                                </div>

                                <div class="tab-pane" id="tab3">

                                    <div class="main_body" id="SubmitSucceful">
                                        <h2 style="font-size:20px;"></h2>

                                    </div>

                                    <div class="row-fluid">
                                        <div>
                                            <span id="lblRemark"> <span></span>
                                            <span id="lblOrderNO"></span>

                                            <br>
                                            <br>
                                            <div>
                                                <div id="pan_en">

                                                    <ul>
                                                        <li>
                                                            1.For barcode items, please confirm artwork before submitting the order.Once submitted,
                                                            the order will be received by our factory and processed immediately.
                                                        </li>
                                                        <li>**Processed orders cannot be cancelled or changed** </li>
                                                        @*<li>
                                                                2. Payment Terms are ship upon payment. Goods will be delivered after we receive
                                                                your payment.
                                                            </li>
                                                            <li>
                                                                3. If you have any complaints regarding our customer service, delays or product
                                                                quality, please contact our Director Ruth Zhu by mail:<a href="mailto: ruth.zhu@maxim-group.com" style="color:blue">
                                                                    ruth.zhu@maxim-group.com
                                                                </a>. Your feedback is very important to us.
                                                            </li>*@
                                                    </ul>

                                                </div>

                                                <div id="pan_zh" style="display:none">
                                                    <ul style="list-style: none; width: 960px; padding: 0px; margin: 0 0 10px 25px;">
                                                        <li>1.关于barcode产品，请确认画稿无误后再提交订单。我司车间会在您提交订单之后收到订单并会直接安排生产。 </li>
                                                        <li>&nbsp;&nbsp; 若订单已生产是无法取消或做任何更改。 </li>
                                                        @*<li>2.付款方式：款到发货。收到您的货款后，我司会尽快为您发货。</li>
                                                            <li>
                                                                3.若您对我们业务人员的服务有任何意见或建议，或对我司产品有任何疑问，请联系我司全球业务总监Ruth Zhu，邮箱地址：<a href="mailto: ruth.zhu@maxim-group.com" style="color:blue">
                                                                    ruth.zhu@maxim-group.com
                                                                </a>. 您的反馈对我们来说至关重要！
                                                            </li>*@
                                                    </ul>
                                                </div>

                                            </div>
                                            <br>


                                        </div>

                                    </div>

                                    <div class="row-fluid">
                                        <div class="span4">
                                            <button id="Submit6" class="btn btn-default" type="submit" value="Ordering Now" name="Command">@ViewData["Return to Shopping"]</button>
                                        </div>
                                        <div class="span4">

                                        </div>
                                        <div class="span4">

                                        </div>
                                    </div>
                                </div>
                                <div class="tab-pane" id="tab4">
                                    <h3 class="block">Confirm your account</h3>
                                </div>

                            </div>

                        </div>

                    </div>

                </div>

            </div>

        </div>

    </div>



    <input type="hidden" id="Language" name="Language" />

    <div id="divimgpreview" style="float: right; position: absolute; display: none; border: 0px solid white;
        z-index: 1000; width: 500px; height: auto; overflow: hidden;">
        <img id="imgpreview" src="" alt="preview" style="border-width: 2px; border: 2px Solid Black;
            border-color: Black; float: right" />
    </div>
</body>
</html>
        


