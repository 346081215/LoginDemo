        function funcDeleteShipTo(obj)
        {
            $("#labtxt").html("Are you sure to Delete?");
            showDiv2();
            //console.log(obj)
            var Customer_ShipToId = obj.value;
            var CustomerId = $("#CustomerRT").val();
            var data_ = {
                CustomerId: CustomerId,
                Customer_ShipToId: Customer_ShipToId
            }
            //console.log(Customer_ShipToId)
            //console.log(CustomerId)
            $("#ok").bind("click",function () {
                $.post("/Maintain/DeleteCustomer_ShipTo", data_, function (data) {
                    if (data.FunStatus) {
                        $("#ShipTo").empty();
                        if (data.DATA.ShipToList==null)
                        {
                            closeDiv();
                        }
                        else
                        {
                            $.each(data.DATA.ShipToList, function (i, item) {
                                var Countrylist = "";
                                Countrylist = "<option value='" + item.Value + "'>" + item.Text + "</option>";
                                $("#ShipTo").append(Countrylist);
                            })

                            var sTrHtml = "";
                            $.each(data.DATA.Shipto, function (i, item) {
                                sTrHtml += "<tr>";
                                sTrHtml += "<td class=center>" + item.Seq + "</td>"
                                sTrHtml += "<td class=center>" + item.CustomerCode + "</td>";
                                sTrHtml += "<td class=center>" + item.CustomerName + "</td>";
                                sTrHtml += " <td class=center><button type=button id=DeleteShipTo name=DeleteShipTo onclick='return funcDeleteShipTo(this)' class='btn btn-default' value='" + item.ID + "'>Delete</button><button type=button id=ShipToHiden name=ShipToHiden class='btn btn-default' style=display:none  value='" + item.ShipID + "'>Edit</button></td>";
                                sTrHtml += "<td class=center><button type=button id=NewShipToAddress name=NewShipToAddress onclick='return funcNewShipToAddress(this)' class='btn btn-default' value='" + item.ShipID + "'>NewAddress</button></td>";
                                sTrHtml += "</tr>";
                            })
                            $("#Shiptos tbody").html(sTrHtml);
                            $("#ShipToIds").val("");
                            closeDiv();
                        }
                       

                    } else {
                        alert("出错了");
                    }
                })
            });
            $("#no").bind("click", function () {
                closeDiv();
            });
          
        }
        function funSaveNewShipto()
        {
            var Isdefault = $("#EditIsdefault input:checked").val();
            var Brandid = $("#Brandid").val();
            var Customer = $("#CustomerRT").val();
            var ShiptoNo = $("#EditAccount").val();
            if (ShiptoNo == "")
            {
                $("#labtxt").html("The Shipto# can not empty.");
                showDiv();
                return;
            }
            var ShiptoName = $("#EditName").val();
            if (ShiptoName == "")
            {
                $("#labtxt").html("The ShiptoName can not empty.");
                showDiv();
                return;
            }
            var _data =
                {
                    Customer: Customer,
                    ShipToNo: ShiptoNo,
                    ShiptoName: ShiptoName,
                    Brandid: Brandid,
                    Isdefault:Isdefault
                }
            $.post("/Maintain/SaveShiptoRT", _data, function (data)
            {
                if (data.FunStatus=="success")
                {
                    var sTrHtml = "";
                    $.each(data.Shipto, function (i, item) {
                        sTrHtml += "<tr>";
                        sTrHtml += "<td class=center>" + item.Seq + "</td>"
                        sTrHtml += "<td class=center>" + item.CustomerCode + "</td>";
                        sTrHtml += "<td class=center>" + item.CustomerName + "</td>";
                        sTrHtml += " <td class=center><button type=button id=DeleteShipTo name=DeleteShipTo onclick='return funcDeleteShipTo(this)' class='btn btn-default' value='" + item.ID + "'>Delete</button><button type=button id=ShipToHiden name=ShipToHiden class='btn btn-default' style=display:none  value='" + item.ShipToid + "'>Edit</button></td>";
                        sTrHtml += "<td class=center><button type=button id=NewShipToAddress name=NewShipToAddress onclick='return funcNewShipToAddress(this)' class='btn btn-default' value='" + item.ShipID + "'>NewAddress</button></td>";
                        sTrHtml += "</tr>";
                    })
                    $("#Shiptos tbody").html(sTrHtml);
                    $("#ShipToIds").val("");
                    $("#EditAccountShipto").css("display","none");
                }
                else
                {
                    $("#labtxt").html(data.FunStatus);
                    showDiv();
                }


            });

        }
        function funcNewShipToAddress(obj)
        {
            $("#oos_name").val("");
            $("#oos_tel").val("");
            $("#oos_EnglishName").val("");
            $("#oos_Address").val("");
            $("#oos_Receiver").val("");
            $("#CheckCountry").empty();
            $("#DeliverryCompany").empty();
            $("#CheckPrintshop").empty();
            var ShipToId = obj.value;
            $("#NewShiptoID").val(ShipToId);
            $("#ShiptoIdNew").val(ShipToId);
            $("#EditAccountShipto").css("display", "none");
            $(".ShiptoAddressDetail").css("display", "none");
            var BrandId = $("#Brandid").val();
            var _data = {
                ShipToId: ShipToId,
                BrandId:BrandId
            }
            $.post("/Maintain/GetShipToAddress", _data, function (data)
            {
                debugger;
                if (data.FunStatus=="success")
                {
                    var sTrHtml = "";
                    $.each(data.ShiptoAddress, function (i, item) {
                        sTrHtml += "<tr>";
                        sTrHtml += "<td class=center>" + item.Seq + "</td>"
                        sTrHtml += "<td class=center>" + item.oos_name + "</td>";
                        sTrHtml += "<td class=center>" + item.oos_address + "</td>";
                        sTrHtml += "<td class=center>" + item.oos_contactname + "</td>";
                        sTrHtml += "<td class=center>" + item.oos_contacttel + "</td>";
                        sTrHtml += "<td class=center>" + item.DeliveryCompany + "</td>";
                     
                        if (item.oos_isdefaultaddr == "True")
                        {
                            sTrHtml += "<td class=center>" + "Yes" + "</td>";
                        }
                        else
                        {
                            sTrHtml += "<td class=center>" + "No" + "</td>";
                        }

                        sTrHtml += "</tr>";
                    })

                    $.each(data.ShippingCompanyList, function (i, item) {
                        var ShippingCompanyList = "";
                        ShippingCompanyList = "<option value='" + item.Value + "'>" + item.Text + "</option>";
                        $("#DeliverryCompany").append(ShippingCompanyList);
                    })
                    $.each(data.ShipmentType, function (i, item) {
                        var ShippingCompanyList = "";
                        ShippingCompanyList = "<option value='" + item.Value + "'>" + item.Text + "</option>";
                        $("#ShipmentType").append(ShippingCompanyList);
                    })
                    $.each(data.TypeofPayment, function (i, item) {
                        var ShippingCompanyList = "";
                        ShippingCompanyList = "<option value='" + item.Value + "'>" + item.Text + "</option>";
                        $("#Typeofpayment").append(ShippingCompanyList);
                    })
                    $.each(data.CountryList, function (i, item) {
                        var Countrylist = "";
                        Countrylist = "<option value='" + item.Value + "'>" + item.Text + "</option>";
                        $("#CheckCountry").append(Countrylist);
                    })
                    $.each(data.PrintShopList, function (i, item) {
                        var PrintShopList = "";

                        if (item.Disabled == false) {
                            $('#CheckPrintshop').append("<option value='" + item.Value + "' style='color:black; font-weight:700'>" + item.Text + "</option>");
                        }
                        else {
                            $('#CheckPrintshop').append("<option value='" + item.Value + "' disabled='disabled' >" + item.Text + "</option>");
                        }
                        $("#CheckPrintshop").append(PrintShopList);
                    });

                    $("#ShiptoAddress tbody").html(sTrHtml);
                    $("#ShiptoAddress").removeAttr("style");
                    $("#ShowInsertAdress").removeAttr("style");
                    ShipmentTypeChange();
                }
                else
                {
                    $("#labtxt").html(data.FunStatus);
                    showDiv();
                }


            });
            
        }
        function funSaveShipToAddress()
        {
            var Name = $("#oos_name").val();
            var EnglishName = $("#oos_EnglishName").val();
            var Address = $("#oos_Address").val();
            var Isdefault = $("#AddressIsdefault input:checked").val();
            var Receiver = $("#oos_Receiver").val();
            var Tel = $("#oos_tel").val();
            var DeliverryCompany = $("#DeliverryCompany option:selected").val();
            var countrycode = $("#CheckCountry option:selected").val();
            var PrintShopId = $("#CheckPrintshop option:selected").val();
            var BrandId = $("#Brandid").val();
            var ShipToId = $("#NewShiptoID").val();
            var ShipmentType = $("#ShipmentType option:selected").val();
            var Typeofpayment = $("#Typeofpayment option:selected").val();
            var AccountNumber = $("#edit_AccountNumber").val();

            if (Name == "") {
                $("#labtxt").html("The Account Name can not empty.");
                showDiv();
                return;
            }
            if (Address == "") {
                $("#labtxt").html("The Address can not empty.");
                showDiv();
                return;
            }
            if (Tel == "") {
                $("#labtxt").html("The Tel can not empty.");
                showDiv();
                return;
            }
            if (ShipmentType == "Express") {
                if (DeliverryCompany == "") {
                    $("#labtxt").html("Please Select Deliverry Company!");
                    showDiv();
                    return;
                }
                if (Typeofpayment == "") {
                    $("#labtxt").html("Please select a payment method!");
                    showDiv();
                    return;
                }
            }
            if (countrycode == "") {
                $("#labtxt").html("Please Select Country.");
                showDiv();
                return;
            }
            if (PrintShopId == "") {
                $("#labtxt").html("Please Select Printshop.");
                showDiv();
                return;
            }
            var _data =
                {
                    Name:Name,
                    EnglishName: EnglishName,
                    Address: Address,
                    Isdefault: Isdefault,
                    Receiver: Receiver,
                    Tel: Tel,
                    DeliverryCompany: DeliverryCompany,
                    countrycode: countrycode,
                    PrintShopId: PrintShopId,
                    BrandId: BrandId,
                    ShipToId: ShipToId,
                    ShipmentType: ShipmentType,
                    Typeofpayment: Typeofpayment,
                    AccountNumber: AccountNumber
                }
            $.post("/Maintain/NewShiptoAddress", _data, function (data)
            {
                if (data.FunStatus == "success") {
                    var sTrHtml = "";
                    $.each(data.ShiptoAddress, function (i, item) {
                        sTrHtml += "<tr>";
                        sTrHtml += "<td class=center>" + item.Seq + "</td>"
                        sTrHtml += "<td class=center>" + item.oos_name + "</td>";
                        sTrHtml += "<td class=center>" + item.oos_address + "</td>";
                        sTrHtml += "<td class=center>" + item.oos_contactname + "</td>";
                        sTrHtml += "<td class=center>" + item.oos_contacttel + "</td>";
                        sTrHtml += "<td class=center>" + item.DeliveryCompany + "</td>";

                        if (item.oos_isdefaultaddr == "True") {
                            sTrHtml += "<td class=center>" + "Yes" + "</td>";
                        }
                        else {
                            sTrHtml += "<td class=center>" + "No" + "</td>";
                        }

                        sTrHtml += "</tr>";
                    })
                    $("#ShiptoAddress tbody").html(sTrHtml);
                    $(".ShiptoAddressDetail").css("display", "none");
                }
                else
                {
                    $("#labtxt").html(data.FunStatus);
                    showDiv();
                }

            })


        }
        $("#ShipmentType").change(function () {
            ShipmentTypeChange();
        });
        function ShipmentTypeChange() {
            var ShipmentTypeid = $("#ShipmentType").val();
            if (ShipmentTypeid == "Express") {
                $("#DeliverryCompany").attr("disabled", false);
                $("#Typeofpayment").attr("disabled", false);
                //$("#edit_AccountNumber").attr("disabled", false);
            } else {
                $("#DeliverryCompany").val("");
                $("#Typeofpayment").val("");
                $("#edit_AccountNumber").val("");
                $("#DeliverryCompany").attr("disabled", "disabled");
                $("#Typeofpayment").attr("disabled", "disabled");
                //$("#edit_AccountNumber").attr("disabled", "disabled");
            }
        }
    </script>
