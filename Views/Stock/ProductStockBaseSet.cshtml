@using System.Data;
@using Maxim.Model.BaseModel
@model PriceUSDBase
@{
    Layout = null;
}
<script src="~/scripts/jquery-1.10.2.min.js"></script>
<script src="~/scripts/Stock/ProductStockBaseSetCode.js"></script>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ProductStockBaseSet</title>
</head>
<body>
    <div class="row-fluid">
        <div class="span12">
            <div class="portlet box " id="form_wizard_1">
                @*页眉*@
                <div class="portlet-title">
                    <div class="caption">
                        <em class="red-em" id="step1">ProductStockBaseSet</em>
                    </div>
                </div>

                @*页面*@
                <div class="portlet-body form">
                    @*条件*@
                    <div id="Query">
                        <h3 class="form-section" style="text-align:center"></h3>
                        @*第一行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls"></div>
                                </div>
                            </div>
                            <div class="span4 ">
                                <span>PrintShop:</span>
                                <select id="PrintShop" name="PrintShop" class="m-wrap span8">
                                    @foreach (SelectListItem item in Model.PrintShopIdList)
                                    {
                                        if (item.Disabled)
                                        {
                                            <option disabled="disabled" value="@item.Value">@item.Text</option>
                                        }
                                        else
                                        {
                                            <option value="@item.Value" style="color:black;font-weight:bold">@item.Text</option>
                                        }
                                    }
                                </select>
                            </div>
                            <div class="span4 ">
                                <span>ProductERPNO:</span>
                                <select id="ProductERPNO" class="m-wrap span8"></select>
                            </div>
                        </div>
                        @*第二行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls"></div>
                                </div>
                            </div>
                            <div class="span4 ">
                                <span></span>
                                <input type="button" id="btn_Select" name="=btn_Edit" class="btn btn-default" value="Search" style="width:80px;height:35px;margin-left:10px;" />
                                <input type="button" id="btn_Add" name="=btn_Edit" class="btn btn-default" value="Add" style="width:80px;height:35px;" />
                            </div>
                        </div>
                    </div>

                    @*查询*@
                    <div class="portlet-body" id="Gridview">
                        <div id="Code" class="dataTables_wrapper form-inline" role="grid">
                            <table class="table table-striped table-bordered table-hover table-full-width dataTable" id="Code_Table" aria-describedby="sample_2_info">
                                <thead>
                                    <tr role="row">
                                        <th class="center">操作</th>
                                        <th class="center">BrandId</th>
                                        <th class="center">PrintShopId</th>
                                        @*<th class="center">ProductCode</th>*@
                                        <th class="center">ProductERPNO</th>
                                        <th class="center">ProductCustNO</th>
                                        <th class="center">BasePaperCode</th>
                                        @*<th class="center">BasePaperERPNO</th><th class="center">BasePaperCustNO</th><th class="center">StockStartQty</th>*@
                                        <th class="center">StockCurrentQty</th>
                                        <th class="center">UpdateTime</th>
                                        @*<th class="center">Notes</th>*@
                                        @*<th class="center">StockDesc</th>*@
                                    </tr>
                                </thead>
                                <tbody class="OrderDetail" role="alert" aria-live="polite" aria-relevant="all"></tbody>
                            </table>
                            <input type="hidden" hidden="hidden" id="MaxinIdHidden" value="">
                        </div>
                        @* 分页 *@
                        <div class="row-fluid">
                            <div class="span4">
                                <div class="mypage">
                                    <ul>
                                        <li id="PageIndex"><<</li>
                                        <li id="Pageup"><</li>
                                        <li><input type="text" id="nowpage" value="1" /></li>
                                        <li>/</li>
                                        <li><i id="TotalPageCount">0</i></li>
                                        <li id="Pagewown">></li>
                                        <li id="PageLast">>></li>
                                        <li id="PageGO">GO</li>
                                        <li><input id="pc" name="pc" type="hidden" value=1><input id="PageTotalCount" name="PageTotalCount" type="hidden" value=1></li>
                                        @*<li><button type="button" id="CreateNewAccount" name="CreateNewAccount" class="btn btn-default" onclick="funAdd();">Create</button></li>*@
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>

                    @*添加修改*@
                    <div id="InsertCountry" class="row-fluid" style="display:none;">
                        @*第一行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls"></div>
                                </div>
                            </div>
                            <div class="span4 ">
                                <span>BrandId:</span>
                                <select id="BrandId" name="BrandId" class="m-wrap span8"></select>
                            </div>
                            <div class="span4 ">
                                <span>PrintShop:</span>
                                <select id="PrintShopId" name="PrintShopId" class="m-wrap span8">
                                    @foreach (SelectListItem item in Model.PrintShopIdList)
                                    {
                                        if (item.Disabled)
                                        {
                                            <option disabled="disabled" value="@item.Value">@item.Text</option>
                                        }
                                        else
                                        {
                                            <option value="@item.Value" style="color:black;font-weight:bold">@item.Text</option>
                                        }
                                    }
                                </select>
                            </div>
                        </div>
                        @*第二行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls"></div>
                                </div>
                            </div>
                            <div class="span4 ">
                                <span>ProductCode:</span>
                                <select id="ProductCode" name="ProductCode" class="m-wrap span8"></select>
                            </div>
                            <div class="span4 ">
                                <span>ProductERPNO:</span>
                                <input type="text" id="Product" name="Product" class="m-wrap span8" disabled />
                            </div>
                        </div>
                        @*第三行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls"></div>
                                </div>
                            </div>
                            <div class="span4 " style="display:none">
                                <span>ProductCustNO:</span>
                                <input type="text" id="ProductCustNO" name="ProductCustNO" class="m-wrap span8" disabled />
                            </div>
                            <div class="span4 ">
                                <span>BasePaperCode:</span>
                                <input type="text" id="BasePaperCode" name="BasePaperCode" class="m-wrap span8" disabled />
                            </div>
                            <div class="span4 ">
                                <span>BasePaperERPNO:</span>
                                <input type="text" id="BasePaperERPNO" name="BasePaperERPNO" class="m-wrap span8" disabled />
                            </div>
                        </div>
                        @*第四行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls"></div>
                                </div>
                            </div>
                           
                            <div class="span4 ">
                                <span>BasePaperCustNO:</span>
                                <input type="text" id="BasePaperCustNO" name="BasePaperCustNO" class="m-wrap span8" disabled />
                            </div>
                            <div class="span4 ">
                                <span>StockStartQty:</span>
                                <input type="text" id="StockStartQty" name="StockStartQty" class="m-wrap span8" />
                            </div>
                        </div>
                        @*第五行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls"></div>
                                </div>
                            </div>
                            
                            <div class="span4 " style="display:none">
                                <span>Notes:</span>
                                <input type="text" id="Notes" name="Notes" class="m-wrap span8" />
                            </div>
                        </div>
                        @*第六行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls"></div>
                                </div>
                            </div>
                            <div class="span4 " style="display:none">
                                <span>StockDesc:</span>
                                <input type="text" id="StockDesc" name="StockDesc" class="m-wrap span8" disabled />
                            </div>
                            <div class="span4 ">
                                
                            </div>
                            <div class="span4 ">
                                <span></span>
                                <input type="button" id="Stock_Add" name="=Stock_Add" class="btn btn-default" value="Save" style="width:80px;height:35px;float:left;margin-left:30px;" />
                                <input type="button" id="Stock_Cancel" name="=Stock_Cancel" class="btn btn-default" value="Cancel" style="width:80px;height:35px;float:left;" />
                                <input type="hidden" id="UpdateId" />
                            </div>
                        </div>
                    </div>

                    <div id="UpdateCountry" class="row-fluid" style="display:none;">
                        @*第一行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls"></div>
                                </div>
                            </div>
                            <div class="span4 ">
                                <span>Brand:</span>
                                @*<select id="BrandType" name="BrandType" class="m-wrap span8"></select>*@
                                @Html.DropDownListFor(x => x.Brandid, Model.BrandList, new { @class = "m-wrap span8" })
                            </div>
                            <div class="span4 ">
                                <span>PrintShop:</span>
                                @*<input type="text" id="PrintShop3"  value="" class="m-wrap span8" readonly="readonly"/>*@
                                <select id="PrintShopId2" name="PrintShopId2" class="m-wrap span8">
                                    @foreach (SelectListItem item in Model.PrintShopIdList)
                                    {
                                        if (item.Disabled)
                                        {
                                            <option disabled="disabled" value="@item.Value">@item.Text</option>
                                        }
                                        else
                                        {
                                            <option value="@item.Value" style="color:black;font-weight:bold">@item.Text</option>
                                        }
                                    }
                                </select>
                                    
                            </div>
                        </div>
                        @*第二行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls"></div>
                                </div>
                            </div>
                            <div class="span4 ">
                                <span>StockType(类型):</span>
                                <select id="OperateType" name="OperateType" class="m-wrap span8">
                                    <option>--All--</option>
                                    <option>material/finished goods in(入库)</option>
                                    <option>Adjusting(调整)</option>
                                </select>

                            </div>
                            <div class="span4 ">
                                <span>Notes(备注):</span>
                                <select id="OperateTypeSub_cnen" name="OperateTypeSub_cnen" class="m-wrap span8"></select>
                            </div>
                        </div>
                        @*第三行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls"></div>
                                </div>
                            </div>
                            <div class="span4 ">
                                <span>ProductCustNO:</span>
                                <input type="text" id="ProductCustNOUp" name="ProductCustNOUp" class="m-wrap span8" readonly="readonly" />
                            </div>
                            <div class="span4 ">
                                <span>ProductERPNO:</span>
                                <input type="text" id="ProductERPNOUp" name="ProductERPNOUp" class="m-wrap span8" readonly="readonly" />
                            </div>
                        </div>
                        @*第四行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls"></div>
                                </div>
                            </div>
                            <div class="span4 ">
                                <span>RefNO(相关单号):</span>
                                <input type="text" id="RefNO" name="RefNO" class="m-wrap span8" />
                            </div>
                            <div class="span4 ">
                                <span>QTY:</span>
                                <input type="text" id="QTY" name="QTY" class="m-wrap span8" />
                            </div>
                        </div>
                        @*第五行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls"></div>
                                </div>
                            </div>
                            <div class="span4 ">
                                <span></span>
                                <input type="button" id="Stock_Edit" name="=Stock_Edit" class="btn btn-default" value="Save" style="width:80px;height:35px;float:left;" />
                                <input type="button" id="Stock_Cancel_Edit" name="=Stock_Cancel" class="btn btn-default" value="Cancel" style="width:80px;height:35px;float:left;" />
                                <input type="hidden" id="UpdateId" />
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</body>
</html>
