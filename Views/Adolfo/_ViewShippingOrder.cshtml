@model IEnumerable<Maxim.Model.BaseModel.ViewShippingOrder>
@using Resources
<div class="page-inner">
    <div class="clearfix main_table" id="Gridview">

        <table class="table table-striped table-bordered table-hover table-full-width dataTable">
            <thead>
                <tr class="head">
                   
                    <th class="center">@RepeatOrder.Seq</th>
                    <th class="center">@RepeatOrder.Maxim</th>
                    <th class="center">@RepeatOrder.Shipment</th>
                    <th class="center">@RepeatOrder.Company</th>
                    <th class="center">@RepeatOrder.Address</th>
                    <th class="center">@RepeatOrder.ShippedDate</th>
                    <th class="center">@RepeatOrder.Contacter</th>
                    <th class="center">@RepeatOrder.Telephone</th>
                    <th class="center">@RepeatOrder.FreightCompany</th>
                    <th class="center">@RepeatOrder.FreightNo</th>
                </tr>
            </thead>
            <tbody class="OrderDetail">
                @foreach (var item in Model)
            {
                    <tr>
                        <td class="center">
                            @Html.DisplayFor(modelItem => item.Seq)
                        </td>
                        <td class="center">
                            @Html.DisplayFor(modelItem => item.MaximNO)
                        </td>
                        <td class="center">
                            @Html.DisplayFor(modelItem => item.ShipOrderNO)
                        </td>
                        <td class="center">
                            @Html.DisplayFor(modelItem => item.Company)
                        </td>
                        <td class="center">
                            @Html.DisplayFor(modelItem => item.Address)
                        </td>
                        <td class="center">
                            @Html.DisplayFor(modelItem => item.ShippedDate)
                        </td>
                        <td class="center">
                            @Html.DisplayFor(modelItem => item.Contacter)
                        </td>
                        <td class="center">
                            @Html.DisplayFor(modelItem => item.Telephone)
                        </td>
                        <td class="center">
                            @Html.DisplayFor(modelItem => item.FreightCompany)
                        </td>
                        <td class="center">
                            <a href="@Html.DisplayFor(modelItem => item.FreightCompanyUrl)" target="_blank">@Html.DisplayFor(modelItem => item.FreightNo)</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>

    </div>
</div>
