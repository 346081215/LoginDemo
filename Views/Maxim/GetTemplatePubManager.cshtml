@using Maxim.Model.BaseModel
@{
    ViewBag.Title = "程序文件发布";
    Layout = null;
}
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>OOSRptPubManager</title>
    <script src="~/scripts/Maxim/GetTemplatePubManager.js"></script>
</head>
<body>
    <div class="row-fluid">
        <div class="span12">
            <div class="portlet box" id="form_wizard_1">
                <div class="portlet-title">
                    <div class="caption">
                        <em class="red-em" id="step1">品牌及模板类别选择</em>
                    </div>
                    
                </div>
                <div class="portlet-body form">
                    <div id="Query-order">
                        <h3 class="form-section" style="text-align:center"></h3>
                        @*第一行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls">
                                    </div>
                                </div>
                            </div>
                            <div class="span4 ">
                                <span>品牌:</span>
                                <select id="Brand" name="Brand" class="m-wrap span8"></select>
                            </div>
                            
                        </div>
                        @*第二行*@
                        <div class="row-fluid">
                            <form action="/Maxim/OnUploadTemplate" method="post" enctype="multipart/form-data" id="FormUpload">
                                <div class="span2 span2_hide">
                                    <div class="control-group">
                                        <label class="control-label" for="firstName"></label>
                                        <div class="controls">
                                        </div>
                                    </div>
                                </div>

                               

                                <div class="span4 ">
                                    <span>文件:</span>
                                    <input id="filename" type="file" name="filelist" value="浏览..." class="m-wrap span8" style="border: none;" />

                                </div>
                            </form>
                        </div>
                        @*第三行*@
                        <div class="row-fluid">
                            <div class="span2 span2_hide">
                                <div class="control-group">
                                    <label class="control-label" for="firstName"></label>
                                    <div class="controls">
                                    </div>
                                </div>
                            </div>
                            <div class="span4">
                                <span></span>
                                <button id="Submit2" class="btn btn-default" type="button" value="Ordering Now" name="Command">Publish</button>
                            </div>
                        </div>
                    </div>

                    <div class="portlet-body" id="Query-Results">
                        <div id="Gridview" class="dataTables_wrapper form-inline" role="grid">
                            <table class="table table-striped table-bordered table-hover table-full-width dataTable" id="Order" aria-describedby="sample_2_info">
                                <thead>
                                    <tr role="row">
                                        <th class="center">FileCode</th>
                                        <th class="center">FileName</th>
                                        <th class="center">FileSysName</th>
                                        <th class="center">FileSysPath</th>
                                        <th class="center">FileActionDesc</th>
                                        <th class="center">Notes</th>
                                        <th class="center">CreatedOn</th>
                                        <th class="center">CreatedBy</th>
                                        <th class="center">BackUpFileName</th>
                                </thead>
                                <tbody class="OrderDetail" role="alert" aria-live="polite" aria-relevant="all"></tbody>
                            </table>
                        </div>
                     </div>
                 </div>
            </div>
        </div>
    </div>
</body>
</html>

    
